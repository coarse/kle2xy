#!/usr/bin/env python3
from kle2xy import KLE2xy

numpad_code = """["NL","/","*","-"],
["7","8","9",{h:2},"+"],
["4","5","6"],
["1","2","3",{h:2},"Enter"],
[{w:2},"0","."]
"""

layout = KLE2xy(numpad_code)

for row in layout:
    for key in row:
        print('%(name)s:%(x)s,%(y)s\t' % key, end="")
    print()
